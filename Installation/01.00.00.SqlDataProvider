/** Create Tables **/
IF NOT EXISTS (SELECT * FROM dbo.sysobjects WHERE id = object_id(N'{databaseOwner}[{objectQualifier}BBStore_Cart]') and OBJECTPROPERTY(id, N'IsTable') = 1)
   BEGIN
      CREATE TABLE {databaseOwner}[{objectQualifier}BBStore_Cart] ( 
         CartID UNIQUEIDENTIFIER NOT NULL,
         PortalID INT NOT NULL,
         SubscriberID INT NOT NULL DEFAULT (0) ,
         CustomerID INT NULL,
         DeliveryAddressID INT NOT NULL DEFAULT (0) ,
         BillingAddressID INT NOT NULL DEFAULT (0) ,
         CustomerPaymentProviderID INT NULL,
         Comment NVARCHAR(500) NULL,
         Currency NVARCHAR(5) NOT NULL DEFAULT N'EUR' ,
         Total DECIMAL(12,4) NOT NULL DEFAULT (0.0000) 
      )
      ALTER TABLE {databaseOwner}[{objectQualifier}BBStore_Cart] ADD CONSTRAINT PK_{objectQualifier}BBStore_Cart PRIMARY KEY NONCLUSTERED  (CartID ASC) WITH ( IGNORE_DUP_KEY = OFF)
   END
GO

