		INSERT INTO {databaseOwner}[{objectQualifier}BBStore_PaymentProvider] ([PaymentProviderID],[ProviderLogo],[ProviderTag],[ProviderControl])VALUES(2,'','CREDITCARD','PaymentProviderCC')
		INSERT INTO {databaseOwner}[{objectQualifier}BBStore_PaymentProvider] ([PaymentProviderID],[ProviderLogo],[ProviderTag],[ProviderControl])VALUES(3,'','COD','PaymentProviderCOD')
